#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        string s;
        int n,r=0,p=0;
        cin>>n;
        cin>>s;
        string s1;
        s1[p]=s[0];
        p=p+1;
        s1[p]=s[1];
       while(p<s.size()-1)
       {
           for(int i=p+1;i<s.size();i++)
       {
           if(s[i]==s1[0])
           {
               if(s[i+1]==s1[1])
               {
                   r=1;
                   break;
               }
           }
       }
       if(r==1)
        break;
       else
       {

           s1[0]=s[p];
           p++;
           s1[1]=s[p];

       }
       }
       if(r==1)
        cout<<"YES"<<endl;
       else cout<<"NO"<<endl;
    }
    return 0;
}
#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        string s;
        int n,r=0,p=0;
        cin>>n;
        cin>>s;
        string s1;
        s1[p]=s[0];
        p=p+1;
        s1[p]=s[1];
       while(p<s.size()-1)
       {
           for(int i=p+1;i<s.size();i++)
       {
           if(s[i]==s1[0])
           {
               if(s[i+1]==s1[1])
               {
                   r=1;
                   break;
               }
           }
       }
       if(r==1)
        break;
       else
       {

           s1[0]=s[p];
           p++;
           s1[1]=s[p];

       }
       }
       if(r==1)
        cout<<"YES"<<endl;
       else cout<<"NO"<<endl;
    }
    return 0;
}
